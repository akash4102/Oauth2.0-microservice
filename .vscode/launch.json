{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Server (ts-node)",
        "runtimeArgs": [
          "-r",
          "ts-node/register"
        ],
        "args": [
          "${workspaceFolder}/src/server.ts"
        ],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "skipFiles": ["<node_internals>/**"],
        "envFile": "${workspaceFolder}/.env",
        "console": "integratedTerminal",
        "runtimeExecutable": "node"
      }
    ]
  }
  